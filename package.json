{
  "name": "ai-job-agent",
  "version": "1.0.0",
  "description": "AI-powered job hunting agent with resume analysis and job matching",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite --config client/vite.config.js",
    "build": "vite build --config client/vite.config.js",
    "preview": "vite preview --config client/vite.config.js",
    "scrape": "node server/scrapers/jobScraper.js"
  },
  "keywords": ["ai", "jobs", "agent", "resume", "career"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.0",
    "react-router-dom": "^6.8.0",
    "lucide-react": "^0.263.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "node-cron": "^3.0.3",
    "fs-extra": "^11.1.1",
    "mongoose": "^8.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "vite": "^4.4.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}